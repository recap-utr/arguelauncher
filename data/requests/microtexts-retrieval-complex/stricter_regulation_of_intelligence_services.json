    "50185290-ecc0-11ec-bc75-5d1586fcf929": {
      "atom": {
        "text": "The German services and others do collaborate with CIA and MI6 closely..."
      },
      "metadata": {
        "created": "2022-06-15T17:32:06Z",
        "updated": "2022-06-15T17:32:06Z"
      }
    },
    "4f00a100-ecc0-11ec-bc75-5d1586fcf929": {
      "atom": {
        "text": "Intelligence services must urgently be regulated more tightly by parliament."
