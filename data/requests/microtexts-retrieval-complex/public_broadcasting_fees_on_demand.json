    "79efca50-ebec-11ec-bc75-5d1586fcf929": {
      "atom": {
        "text": "We do want to get our news through the lense of neutral and independent journalists."
      },
      "metadata": {
        "created": "2022-06-14T16:15:43Z",
        "updated": "2022-06-14T16:15:43Z"
      }
    },
    "7df80c20-ebec-11ec-bc75-5d1586fcf929": {
      "atom": {
        "text": "The people should be able to decide on their own about the program, they want to pay for."
      },
      "metadata": {
        "created": "2022-06-14T16:15:49Z",
        "updated": "2022-06-14T16:15:49Z"
      }
    },
    "9c2ff2c0-ebec-11ec-bc75-5d1586fcf929": {
      "atom": { "text": "Because the need for independent media is high. " },
      "metadata": {
        "created": "2022-06-14T16:16:40Z",
        "updated": "2022-06-14T16:16:40Z"
      }
    }
