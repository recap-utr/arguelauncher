    "7c7b0910-ebec-11ec-bc75-5d1586fcf929": {
      "atom": { "text": "Everybody should pay for public broadcasters." },
      "metadata": {
        "created": "2022-06-14T16:15:47Z",
        "updated": "2022-06-14T16:15:47Z"
      }
    },
    "7b591e50-ebec-11ec-bc75-5d1586fcf929": {
      "atom": {
        "text": "There are a lot of programmes, that are not worth the money we paid."
      },
      "metadata": {
        "created": "2022-06-14T16:15:45Z",
        "updated": "2022-06-14T16:15:45Z"
      }
    },
