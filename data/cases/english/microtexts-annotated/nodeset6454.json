    {
      "id": 120752,
      "x": 565,
      "y": 85,
      "color": "m",
      "text": "Considering what's on television these days, it would be appropriate to only have those people pay who actually (want to) follow the programming.",
      "text_begin": [],
      "text_end": [],
      "text_length": [
        145
      ],
      "comment": "",
      "type": "I",
      "scheme": "0",
      "descriptors": {},
