    },
    {
      "id": 120753,
      "x": 821,
      "y": 379,
      "color": "b",
      "text": "In some cases there are only shows for older generations on at prime times, so why then should everyone pay for this?",
      "text_begin": [],
      "text_end": [],
      "text_length": [
        117
      ],
      "comment": "",
      "type": "I",
      "scheme": "0",
