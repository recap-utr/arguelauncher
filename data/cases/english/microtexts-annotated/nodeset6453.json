    {
      "id": 120742,
      "x": 1199,
      "y": 279,
      "color": "b",
      "text": "If there were capital punishment, this would mean people are allowed to put themselves above others and rule over their lives.",
      "text_begin": [],
      "text_end": [],
      "text_length": [
        126
      ],
      "comment": "",
      "type": "I",
      "scheme": "0",
      "descriptors": {},
